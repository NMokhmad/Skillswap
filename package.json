  "type": "module",
  "bugs": {
    "url": "https://github.com/O-clock-Raclette/skill-swap/issues"
  },
  "homepage": "https://github.com/O-clock-Raclette/skill-swap#readme",
  "devDependencies": {
    "@eslint/js": "^9.20.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.20.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "sequelize-cli": "^6.6.5"
  },
  "dependencies": {
    "argon2": "^0.41.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "ejs": "^3.1.10",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "method-override": "^3.0.0",
    "multer": "^2.0.2",
    "pg": "^8.13.1",
    "sanitize-html": "^2.17.0",
    "sequelize": "^6.37.5"
  }
}
